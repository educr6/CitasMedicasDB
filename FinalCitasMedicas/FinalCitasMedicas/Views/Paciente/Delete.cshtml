@model FinalCitasMedicas.tblPaciente

@{
    ViewBag.Title = "Delete";
}


<hr style="border-width: 5px;" />
<h3>Delete</h3>
<hr style="border-width: 5px;" />


<div>
    <h4>Detalle Paciente </h4>

    <div>
        <p> Nombre : @Html.TextBoxFor(model => model.nombrePaciente)</p>
        <p> Cedula : @Html.TextBoxFor(model => model.cedulaPaciente) </p>
        <p> Correo : @Html.TextBoxFor(model => model.correoPaciente)</p>
        <p> Telefono : @Html.TextBoxFor(model => model.telefonoPaciente)</p>
        <p> Direccion : @Html.TextBoxFor(model => model.direccionPaciente)</p>

    </div>
    <hr style="border-width: 5px;" />

    @Html.HiddenFor(m => m.idPaciente)

    <h4>Estas seguro que quiere Eliminar este Paciente?</h4>

    @using (@Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-actions danger">
            <input type="submit" value="Eliminar" class="btn btn-danger" />
            |
            @Html.ActionLink("Regresar", "Index")
        </div>
    }
</div>
